library(shinydashboard)
library(ggplot2)
library(dplyr)
library(leaflet)
library(jsonlite)
source("functions.R")
nba_df = read.csv("nba.csv")
nba_df$date <- as.Date(nba_df$date)
# Interface
ui <- dashboardPage(
skin = 'blue',
dashboardHeader(
title = h4(HTML("Estadísticas NBA<br/>2018 - 2019"))
),
dashboardSidebar(
sidebarMenu(
menuItem('Estadísticas Semanales', tabName = 'Temporal'),
menuItem('Salario por provincias', tabName = 'Provincias')
)),
dashboardBody(
tabItems(
tabItem(tabName = 'Temporal',
fluidRow(
column(width = 4,
wellPanel(selectInput(inputId = 'team_weekly',
label = 'Equipo',
choices = c("TODOS", unique(nba_df$team_name)),
multiple = FALSE))
),
column(width = 6,
dygraphOutput("dygraph", height = 500)
)
)
),
tabItem(tabName = 'Provincias',
fluidRow(
column(width = 4,
wellPanel(checkboxGroupInput(inputId = 'sexos',
label = 'Sexo',
choices = c('Hombres' = 'hombre', 'Mujeres' = 'mujer'),
selected = c('hombre', 'mujer'))),
wellPanel(checkboxGroupInput(inputId = 'contratos',
label = 'Tipo de contrato',
choices = c('Indefinido', 'Temporal'),
selected = c('Indefinido', 'Temporal')))
),
column(width = 6,
leafletOutput(outputId = 'mapa'))
)
)
)
)
)
# Server
server <- function(input, output){
output$dygraph <- renderDygraph({
draw_dygraph(input$team_weekly)
})
}
# Deployment
shinyApp(ui = ui, server = server)
